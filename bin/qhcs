p1=$1
shift
ghc --make -package qt -fglasgow-exts -XBangPatterns -XRecordWildCards -XNamedFieldPuns -O2 -o $p1 $p1.hs $* -i.
if [ -f $p1 ]
then
	strip $p1
fi

